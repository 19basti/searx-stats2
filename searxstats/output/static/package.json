{
  "private": true,
  "name": "searx-space",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "per-env",
    "start:production": "npm run -s serve",
    "start:development": "npm run -s dev",
    "watch": "preact watch --prerenderUrls ./prerender-urls.js --sw false",
    "build": "preact build --prerenderUrls ./prerender-urls.js --sw false --production",
    "serve": "preact build --prerenderUrls ./prerender-urls.js --sw false --production && sirv build --cors --single --port 8888 --etag --host 0.0.0.0",
    "lint": "eslint src",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^2.2.0",
    "eslint": "^7.2.0",
    "eslint-config-preact": "^1.1.1",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "jest-preset-preact": "^1.0.0",
    "node-sass": "^4.13.1",
    "per-env": "^1.0.2",
    "preact-cli": "^3.0.0-rc.14",
    "preact-cli-plugin-critical-css": "^0.1.0",
    "preact-render-spy": "^1.3.0",
    "sass-loader": "^8.0.2",
    "sirv-cli": "^0.4.6",
    "webpack": "^4.42.1"
  },
  "dependencies": {
    "@popperjs/core": "^2.4.2",
    "@preact/prerender-data-provider": "^3.0.0-rc.9",
    "bootstrap": "^4.5.0",
    "fibers": "^4.0.2",
    "linkstate": "^1.1.1",
    "node-fetch": "^2.6.0",
    "preact": "^10.4.4",
    "preact-render-to-string": "^5.1.9",
    "preact-router": "^3.2.1",
    "react-popper": "^2.2.3",
    "sass": "^1.26.8",
    "unistore": "^3.5.1"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.1",
    "utf-8-validate": "^5.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
